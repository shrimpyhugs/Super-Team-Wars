if (xs &lt;= margin or keyboard_check(vk_left)) and view_xview[0] &gt; -30 { //if the mouse is on the left of screen
view_xview[0] -= 10 //move the view to the left
}
else if(xs &gt;= (view_wview[0] - margin) or keyboard_check(vk_right)) and (view_xview[0] + view_wview[0] &lt; room_width + 30) { //if the mouse is on the right of screen
view_xview[0] += 10 //move the view to the right
}

if(ys &lt;= margin or keyboard_check(vk_up)) and view_yview[0] &gt; -30 { //if the mouse is at the top of the screen
view_yview[0] -= 10 //move the view up
}
else if(ys &gt;= (view_hview[0] - margin) or keyboard_check(vk_down)) and (view_yview[0] + view_hview[0] &lt; room_height + 30) { //if the mouse is at the bottom of the screen
view_yview[0] += 10 //move the view down
}

///Update GUI Mouse Position Code
